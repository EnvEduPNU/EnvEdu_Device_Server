{
    "family": "EnvEduDeviceFamily",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::221523387111:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::221523387111:role/ecsTaskRole",
    "containerDefinitions": [
        {
            "name": "EnvEduDeviceContainer",
            "image": "221523387111.dkr.ecr.ap-northeast-2.amazonaws.com/envedu_device_repo:latest",
            "cpu": 256,
            "memory": 512,
            "essential": true,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/EnvEduDeviceFamily",
                    "awslogs-region": "ap-northeast-2",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "1024",
    "memory": "3072",
    "volumes": []
}
