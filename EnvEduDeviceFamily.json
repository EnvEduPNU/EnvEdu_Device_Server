{
    "family": "EnvEduDeviceFamily",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::221523387111:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::221523387111:role/ecsTaskRole",
    "containerDefinitions": [
        {
            "name": "EnvEduDeviceContainer",
            "image": "221523387111.dkr.ecr.ap-northeast-2.amazonaws.com/envedu_device_repo:latest",
            "cpu": 256,
            "memory": 512,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080
                }
            ]
        }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "1024",
    "memory": "3072",
    "volumes": []
}
